{
  "name": "shadcn-vue",
  "type": "module",
  "version": "0.10.4",
  "description": "Add components to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/radix-vue/shadcn-vue.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "components",
    "ui",
    "vue",
    "nuxt",
    "tailwind",
    "radix-ui",
    "radix-vue",
    "shadcn",
    "shadcn-vue"
  ],
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "node ./scripts/rimraf.js",
