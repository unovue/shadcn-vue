    "pub:release": "pnpm build && pnpm publish  --no-git-checks --access public",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "@vitest/ui": "*",
    "vitest": "*"
  },
  "dependencies": {
